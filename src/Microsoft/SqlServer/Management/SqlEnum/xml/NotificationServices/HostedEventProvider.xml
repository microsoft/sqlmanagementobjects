		<prefix>
CREATE TABLE #tmp_hosted_event_provider(
    ProviderId int,
    ProviderName nvarchar(255),
    ProviderClassName nvarchar(255),
    AssemblyName nvarchar(260),
    SystemName nvarchar(255),
    StartTime datetime,
    Interval int,
    Timeout int,
    ActivationState tinyint
)
